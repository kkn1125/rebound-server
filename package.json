{
  "name": "seed-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "seed": "ts-node prisma/seed.ts",
    "seed:mockdata": "prisma db seed -- --environment development",
    "generate:alias": "ts-node lib/generate.depth.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:prisma": "prisma-case-format -c .prisma-case-format -f prisma/schema.prisma -p",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "coverage": "vitest run --coverage",
    "vitest": "vitest --ui -c ./vitest.config.mjs",
    "vitest:e2e": "vitest -c ./test/vitest.config.mjs"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.4",
    "@prisma/client": "^6.4.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.4.1",
    "prisma-case-format": "^2.2.1",
    "prisma-class-generator": "^0.2.11",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.5.0",
    "@mermaid-js/mermaid-cli": "^11.4.2",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.1",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@vitest/browser": "^3.0.7",
    "@vitest/coverage-istanbul": "^3.0.7",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "playwright": "^1.50.1",
    "prettier": "^3.4.2",
    "prisma-erd-generator": "^2.0.4",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@types/(.*)": [
        "<rootDir>/types/$1"
      ],
      "^@middleware/(.*)": [
        "<rootDir>/middleware/$1"
      ],
      "^@logger/(.*)": [
        "<rootDir>/logger/$1"
      ],
      "^@config/(.*)": [
        "<rootDir>/config/$1"
      ],
      "^@common/(.*)": [
        "<rootDir>/common/$1"
      ],
      "^@/(.*)": [
        "<rootDir>/$1"
      ]
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
